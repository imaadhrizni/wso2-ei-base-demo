<?xml version="1.0" encoding="UTF-8" standalone="no"?><xsl:stylesheet xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:firstElementOfTheInput="Company" xmlns:notXSLTCompatible="false" xmlns:own="http://ownfunction.org" xmlns:runTimeProperties="endpointOneResponse,DEFAULT" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" extension-element-prefixes="xs own runTimeProperties notXSLTCompatible firstElementOfTheInput" version="2.0"><xsl:function name="own:setPrecision"><xsl:param name="resultString"/><xsl:param name="noOfDigits"/><xsl:if test="$noOfDigits=0"><xsl:value-of select="$resultString"/></xsl:if><xsl:if test="$noOfDigits!=0"><xsl:value-of select="own:setPrecision(concat($resultString,'0'),$noOfDigits - 1)"/></xsl:if></xsl:function><xsl:param name="DEFAULT_endpointOneResponse"/><xsl:template match="/"><root><FirstName><xsl:value-of select="Company/Employee/FirstName"/></FirstName><LastName><xsl:value-of select="Company/Employee/LastName"/></LastName><ContactNo><xsl:value-of select="number( Company/Employee/ContactNo )"/></ContactNo><Email><xsl:value-of select="Company/Employee/Email"/></Email><Address><City><xsl:value-of select="Company/Employee/Address/City"/></City><State><xsl:value-of select="Company/Employee/Address/State"/></State><Zip><xsl:value-of select="number( Company/Employee/Address/Zip )"/></Zip></Address><InternalCompanyId><xsl:value-of select="$DEFAULT_endpointOneResponse"/></InternalCompanyId></root></xsl:template></xsl:stylesheet>